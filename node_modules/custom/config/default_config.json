{
  "environment": "development",

  "database": {
    "redis": {
      "communicator": {
        "address": {
          "ip"  : "127.0.0.1",
          "port": 6379
        }
      },
      "archive": {
        "address": {
          "ip"  : "127.0.0.1",
          "port": 6379
        }
      }
    }
  },

  "manager": {
    "address": {
      "ip"  : "127.0.0.1",
      "port": 8080
    },
    "instance": {
      "namePrefix": "test_microservice_"
    },
    "stats": {
      "oldestStatsTimestamp": 864000000  /* 10 days = 10 * 24 * 60 * 60 * 1000 = 864000000 */
    }
  },

  "services": {
    "fibonacci-number": {
      "address": {
        "defaultPort": 4100
      },
      "location": "/getFibonacciNumber",
      "totalInitialInstances": 1,
      "maximumRunningInstances": 10
    },

    "prime-number": {
      "address": {
        "defaultPort": 4200
      },
      "location": "/getPrimeNumber",
      "totalInitialInstances": 1,
      "maximumRunningInstances": 10
    },

    "combo-number": {
      "address": {
        "defaultPort": 4300
      },
      "location": "/getComboNumber",
      "totalInitialInstances": 1,
      "maximumRunningInstances": 10
    },

    "random-string": {
      "address": {
        "defaultPort": 4400
      },
      "location": "/getRandomString",
      "totalInitialInstances": 1,
      "maximumRunningInstances": 10
    }
  }
}